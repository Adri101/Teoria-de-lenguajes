    0 $accept: programa $end

    1 $@1: %empty

    2 $@2: %empty

    3 programa: zona1 $@1 SEPARADOR $@2 zona2

    4 zona1: zona1 definicion
    5      | definicion

    6 definicion: variables
    7           | asignaciones
    8           | sensores
    9           | actuadores
   10           | error

   11 variables: INT ListaVariables
   12          | FLOAT ListaVariables
   13          | POSITION ListaVariables
   14          | STRING ListaVariables

   15 ListaVariables: ID ';'
   16               | ID ',' ListaVariables

   17 asignaciones: ID '=' expr ';'
   18             | ID '=' '<' expr ',' expr '>' ';'
   19             | ID '=' CADENA ';'

   20 sensores: SMOKE ID '<' expr ',' expr '>' CADENA ';'
   21         | LIGHT ID '<' expr ',' expr '>' CADENA ';'
   22         | TEMPERATURE ID ID CADENA ';'

   23 actuadores: ALARM ID '<' expr ',' expr '>' CADENA ';'
   24           | ALARM ID ';'
   25           | SWITCH ID '<' expr ',' expr '>' CADENA ';'
   26           | SWITCH ID ';'
   27           | MESSAGE ID '<' expr ',' expr '>' CADENA ';'
   28           | MESSAGE ID ';'

   29 zona2: escenarios
   30      | zona2 escenarios
   31      | error

   32 $@3: %empty

   33 escenarios: SCENE ID $@3 '[' bloqueInstrucciones ']' ';'

   34 bloqueInstrucciones: bloqueInstrucciones listaEscenarios
   35                    | listaEscenarios

   36 listaEscenarios: ID expr ';'
   37                | START ';'
   38                | PAUSE ';'
   39                | START expr ';'
   40                | PAUSE expr ';'

   41 $@4: %empty

   42 listaEscenarios: IF exprl THEN $@4 '[' bloqueInstrucciones ']' ';'

   43 $@5: %empty

   44 listaEscenarios: ELSE $@5 '[' bloqueInstrucciones ']' ';'
   45                | ID OFF ';'
   46                | ID ON ';'
   47                | ID ON CADENA ';'
   48                | ID OFF CADENA ';'
   49                | ID ON ID ';'
   50                | ID OFF ID ';'

   51 $@6: %empty

   52 listaEscenarios: REPEAT expr $@6 '[' bloqueInstrucciones ']' ';'

   53 expr: ENTERO
   54     | REAL
   55     | ID
   56     | expr '*' expr
   57     | expr '-' expr
   58     | expr '+' expr
   59     | expr '/' expr
   60     | expr '%' expr
   61     | '-' expr
   62     | expr '^' expr

   63 exprl: cmp
   64      | '(' exprl ')'
   65      | '!' exprl
   66      | exprl '&' '&' exprl
   67      | exprl '|' '|' exprl
   68      | VERDAD
   69      | FALSO

   70 cmp: expr '=' '=' expr
   71    | expr '!' '=' expr
   72    | expr '>' expr
   73    | expr '<' expr
   74    | expr '<' '=' expr
   75    | expr '>' '=' expr


